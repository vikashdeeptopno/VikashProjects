<div class="highlight hl-ipython3"><pre><span></span><span class="c1">#Adding New Colums - Age Groups</span>

<span class="n">df</span><span class="p">[</span><span class="s1">'Age Group'</span><span class="p">]</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">cut</span><span class="p">(</span>
    <span class="n">df</span><span class="p">[</span><span class="s1">'Age'</span><span class="p">],</span>
    <span class="n">bins</span> <span class="o">=</span> <span class="p">[</span><span class="mi">18</span><span class="p">,</span> <span class="mi">20</span><span class="p">,</span> <span class="mi">22</span><span class="p">,</span> <span class="mi">24</span><span class="p">,</span> <span class="mi">26</span><span class="p">,</span> <span class="mi">28</span><span class="p">],</span>
    <span class="n">labels</span> <span class="o">=</span> <span class="p">[</span><span class="s2">"18 - 20"</span><span class="p">,</span> <span class="s2">"21 - 22"</span><span class="p">,</span> <span class="s2">"23 - 24"</span><span class="p">,</span> <span class="s2">"25 - 26"</span><span class="p">,</span> <span class="s2">"27 - 28"</span><span class="p">],</span>
    <span class="n">include_lowest</span> <span class="o">=</span> <span class="kc">True</span>
<span class="p">)</span>

<span class="c1">#Risk Levels as per daily usage</span>

<span class="k">def</span><span class="w"> </span><span class="nf">classify_risk</span><span class="p">(</span><span class="n">hours</span><span class="p">):</span>
    <span class="k">if</span> <span class="n">pd</span><span class="o">.</span><span class="n">isna</span><span class="p">(</span><span class="n">hours</span><span class="p">):</span>
        <span class="k">return</span> <span class="s2">"Unknown"</span>
    <span class="k">if</span> <span class="n">hours</span> <span class="o">&lt;</span> <span class="mi">2</span><span class="p">:</span>
        <span class="k">return</span> <span class="s2">"Low Risk"</span>
    <span class="k">elif</span> <span class="n">hours</span> <span class="o">&lt;</span> <span class="mi">5</span><span class="p">:</span>
        <span class="k">return</span> <span class="s2">"Moderate Risk"</span>
    <span class="k">else</span><span class="p">:</span>
        <span class="k">return</span> <span class="s2">"High Risk"</span>

<span class="n">df</span><span class="p">[</span><span class="s1">'risk_level'</span><span class="p">]</span> <span class="o">=</span> <span class="n">df</span><span class="p">[</span><span class="s1">'Avg_Daily_Usage_Hours'</span><span class="p">]</span><span class="o">.</span><span class="n">apply</span><span class="p">(</span><span class="n">classify_risk</span><span class="p">)</span>

<span class="c1">#Addiction category</span>

<span class="k">def</span><span class="w"> </span><span class="nf">addiction_category</span><span class="p">(</span><span class="n">s</span><span class="p">):</span>
    <span class="k">if</span> <span class="n">s</span> <span class="o">&gt;=</span> <span class="mi">8</span><span class="p">:</span>
        <span class="k">return</span> <span class="s2">"Severe"</span>
    <span class="k">if</span> <span class="n">s</span> <span class="o">&gt;=</span> <span class="mi">6</span><span class="p">:</span>
        <span class="k">return</span> <span class="s2">"High"</span>
    <span class="k">if</span> <span class="n">s</span> <span class="o">&gt;=</span> <span class="mi">4</span><span class="p">:</span>
        <span class="k">return</span> <span class="s2">"Moderate"</span>
    <span class="k">return</span> <span class="s2">"Low"</span>

<span class="n">df</span><span class="p">[</span><span class="s2">"addiction_category"</span><span class="p">]</span> <span class="o">=</span> <span class="n">df</span><span class="p">[</span><span class="s2">"Avg_Daily_Usage_Hours"</span><span class="p">]</span><span class="o">.</span><span class="n">apply</span><span class="p">(</span><span class="n">addiction_category</span><span class="p">)</span>

